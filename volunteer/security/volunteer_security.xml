<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="group_volunteer_admin" model="res.groups">
            <field name="name">Volunteer/Admin</field>
        </record>
        <record id="only_active_modify" model="ir.rule">
            <field name="name">Only Active Course Can Be Modified</field>
            <field name="perm_read" eval="0"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="0"/>
            <field name="perm_unlink" eval="1"/>
            <field name="domain_force">[('active','=',True)]</field>
            <field name="model_id" ref="model_volunteer_task"/>
        </record>
    </data>
</odoo>