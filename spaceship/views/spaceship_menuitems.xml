<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.actions.act_window" id="spaceship_list_action">
        <field name="name">Spaceship</field>
        <field name="res_model">spaceship.spaceship</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                 Create the first mission
            </p>
        </field>
    </record>

    <menuitem name="Space-Mission" 
        id="menu_spaceship" 
        sequence="1"
        web_icon="spaceship,static/description/icon.png">
        <menuitem id="menu_spaceship_sub" name="Space-Mission" sequence="1">
            <menuitem id="menu_spaceship_spaceships" name="Spaceships" sequence="2" action="spaceship_list_action"/>
        </menuitem>
    </menuitem>
</odoo> 