<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.actions.act_window" id="book_list_action">
        <field name="name">Book</field>
        <field name="res_model">library.book</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                 Create a book
            </p>
        </field>
    </record>

    <menuitem name="Library Book" 
        id="menu_library" 
        sequence="1"
        web_icon="library,static/description/icon.png">
        <menuitem id="menu_library_sub" name="Library Book" sequence="1">
            <menuitem id="menu_library_books" name="Books" sequence="2" action="book_list_action"/>
        </menuitem>
    </menuitem>
</odoo> 